{
	"npmClient": "npm",
	"pipeline": {
		"build": {
			"outputs": ["src/server/static/scripts/**", "src/server/static/styles/**"],
			"dependsOn": ["^build", "$NODE_ENV", "$APP_RELEASE"],
			"dotEnv": [".env"]
		},
		"build:release": {
			"outputs": ["src/server/static/scripts/**", "src/server/static/styles/**"],
			"dependsOn": ["^build", "$NODE_ENV", "$APP_RELEASE"],
			"dotEnv": [".env.production", ".env"]
		},
		"test": {
			"outputs": ["coverage/**"],
			"dependsOn": [],
			"inputs": [
				"src/**/*.{js,ts}",
				"**/__tests__/**/*.test.js",
				"**/__tests__/__snapshots__/**",
				"jest.config.js",
				"jest.setup.js",
				".env",
				".env.test"
			],
			"dotEnv": [".env.test", ".env"]
		},
		"test:cov": {
			"outputs": ["coverage/**"],
			"dependsOn": [],
			"inputs": [
				"src/**/*.{js,ts}",
				"**/__tests__/**/*.test.js",
				"**/__tests__/__snapshots__/**",
				"jest.config.js",
				"jest.setup.js",
				".env",
				".env.test"
			],
			"dotEnv": [".env.test", ".env"]
		},
		"lint:js": {
			"dependsOn": ["^build"],
			"outputs": []
		},
		"lint:js:fix": {
			"dependsOn": ["^build"],
			"outputs": []
		},
		"prisma-generate": {
			"cache": false
		},
		"tscheck": {
			"dependsOn": []
		},
		"dev": {
			"cache": false,
			"dotEnv": [".env"]
		},
		"clean": {
			"cache": false
		}
	}
}
